on-row {\n  display: flex;\n  justify-content: center;\n}\n\n.button {\n  font-size: 1.2rem;\n  background-color: var(--color-pink);\n  color: white;\n  padding: 1rem 3rem;\n  border-radius: 0.2rem;\n  text-decoration: none;\n  box-shadow: inset 0 0 1px 0 var(--color-violet), 1px 1px 10px 1px #ff52bf44;\n  transition: transform 0.2s ease, background-color 0.2s ease;\n  transform: scale(1) translateY(0);\n  outline: 0;\n  border: 0;\n  &:hover {\n    transform: scale(1.05) translateY(-1px);\n    background-color: lighten(#ff52bf, 1);\n  }\n  @media (min-width: 500px) {\n    padding: 1.3rem 5rem;\n  }\n}\n\n.button.white {\n  background-color: white;\n  box-shadow: inset 0 0 1px 0 var(--color-violet),\n    1px 1px 10px 1px rgba(154, 82, 255, 0.1);\n  color: var(--color-violet);\n}\n\n.card {\n  background-color: white;\n  padding: 4rem 2rem;\n  border-radius: 0.5rem;\n  box-shadow: 1px 1px 20px 3px rgba(154, 82, 255, 0.1);\n  @media (min-width: 768px) {\n    padding: 6rem 4rem;\n  }\n}\n\n.container {\n  max-width: 1000px;\n  margin: 0 auto;\n  width: 90vw;\n}\n\nheader.container {\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 2rem 0;\n  margin-bottom: 5rem;\n  width: 90vw;\n  @media (min-width: 768px) {\n    margin-bottom: 10rem;\n  }\n}\n\n.logo {\n  width: 140px;\n  @media (min-width: 520px) {\n    width: auto;\n  }\n}\n\nmain {\n  position: relative;\n  z-index: 5;\n  display: grid;\n  grid-template-columns: [left-side] 1fr [main-start] 4fr [main-end] 1fr [right-side];\n  grid-template-rows: repeat(5, auto);\n}\n\nheader {\n  position: relative;\n  z-index: 5;\n  grid-row: 1/2;\n}\n\n#hero {\n  grid-row: 2/3;\n  text-align: center;\n  margin-bottom: 20rem;\n  grid-column: left-side / right-side;\n  @media (min-width: 768px) {\n    grid-column: main-start;\n    margin-bottom: 35rem;\n  }\n}\n\n#features {\n  grid-row: 3/4;\n  grid-column: left-side / right-side;\n  display: grid;\n  grid-template-columns: 1fr;\n  grid-template-rows: auto auto;\n  grid-gap: 8rem;\n  align-items: center;\n  margin-bottom: 20rem;\n  text-align: center;\n  @media (min-width: 768px) {\n    grid-template-columns: [image] 1fr [words] 1fr;\n    grid-template-rows: 1fr;\n    text-align: left;\n    margin-bottom: 35rem;\n  }\n}\n\n#try {\n  grid-row: 4/5;\n  grid-column: left-side / right-side;\n  text-align: center;\n  margin-bottom: 20rem;\n  @media (min-width: 768px) {\n    grid-column: main-start;\n    margin-bottom: 35rem;\n  }\n}\n\n#footer {\n  background-color: var(--very-dark-cyan);\n  color: white;\n  padding-top: 6rem;\n  padding-bottom: 6rem;\n  .container {\n    display: grid;\n    grid-template-columns: 1fr;\n    grid-gap: 3rem;\n    grid-template-areas:\n      \"logo\"\n      \"links\"\n      \"form\"\n      \"social\";\n    @media (min-width: 768px) {\n      grid-template-columns: 1fr 1fr 1fr;\n      grid-template-areas:\n        \"logo logo logo\"\n        \"social links form\";\n    }\n  }\n  p,\n  ul,\n  ol,\n  li,\n  a {\n    font-size: 1.4rem;\n  }\n}\n\n.footer-logo {\n  grid-column: 1 / -1;\n  grid-area: logo;\n}\n\n.footer-social {\n  grid-area: social;\n  .social-link {\n    width: 2.5rem;\n    margin-right: 2rem;\n  }\n}\n\n.footer-links {\n  grid-area: links;\n  display: grid;\n  grid-template-columns: 1fr;\n  ul {\n    list-style: none;\n    margin: 0;\n    padding: 0;\n    li {\n      margin-bottom: 1.5rem;\n      a {\n        color: white;\n        margin: 0;\n        text-decoration: none;\n        &:hover {\n          text-decoration: underline;\n        }\n      }\n    }\n    &:last-child {\n      li:list-child {\n        margin-bottom: 0;\n      }\n    }\n  }\n  @media (min-width: 768px) {\n    grid-template-columns: 1fr 1fr;\n    grid-gap: 2rem;\n  }\n}\n\n.footer-form {\n  grid-area: form;\n  input {\n    padding: 0.2rem 0.5rem;\n    width: 100%;\n    border-radius: 0.4rem;\n    border: 0;\n    height: 4rem;\n    font-size: 2rem;\n    margin-bottom: 1.5rem;\n  }\n  .button-row {\n    justify-content: flex-end;\n  }\n  button {\n    cursor: pointer;\n    width: 100%;\n  }\n  @media (min-width: 768px) {\n    button {\n      width: auto;\n    }\n  }\n}\n\n.block {\n  position: absolute;\n  border-radius: 15vw;\n  @media (min-width: 500px) {\n    border-radius: 10vw;\n  }\n}\n\n.block.purple {\n  z-index: 2;\n  top: -8vw;\n  right: -75vw;\n  width: 122vw;\n  height: 90vw;\n  transform: rotate(60deg);\n  background: linear-gradient(var(--color-violet), var(--color-pink));\n  @media (min-width: 720px) {\n    transform: rotate(45deg);\n    top: -45vw;\n    right: -45vw;\n    width: 90vw;\n    height: 90vw;\n  }\n}\n\n.block.white {\n  transform: rotate(-25deg);\n  background: white;\n  top: 8rem;\n  right: -10rem;\n  z-index: 1;\n  width: 125rem;\n  height: 80rem;\n  @media (min-width: 720px) {\n    top: 20rem;\n    right: 3rem;\n  }\n  @media (min-width: 1024px) {\n    right: 15rem;\n  }\n}\n\nfooter {\n  background-color: var(--very-dark-cyan);\n}\n"},"lineCount":null}},"error":null,"hash":"b771fbfffd9e924f61d6a1018b3c54b4","cacheData":{"env":{}}}